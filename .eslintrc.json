{
  "extends": [
    "plugin:adonis/typescriptPackage"
  ],
  "rules": {
    "@typescript-eslint/semi": [2, "always"],
    "@typescript-eslint/space-before-function-paren": [2, "never"],
    "comma-dangle": [1, "never"],
    "object-curly-spacing": [2, "always"],
    "curly": [1, "multi-line"]
  }
}
